<script>
var place =new google.maps.LatLng(61.207676, -149.782375);
var place1 =new google.maps.LatLng (62.634608, -150.287747);
var place2 =new google.maps.LatLng (63.145254, -149.914211);
var place3 =new google.maps.LatLng (63.352947, -150.463528);
var place4 =new google.maps.LatLng(63.578711, -149.804348);
var place5 =new google.maps.LatLng(63.724990, -148.969387);
var place6 =new google.maps.LatLng(62.966041, -149.892239);



function initialize()
{


var mapProp = {
  center:place1,
  zoom:6,
  mapTypeId:google.maps.MapTypeId.ROADMAP
  };

var map=new google.maps.Map(document.getElementById("googleMap"),mapProp);
var hike = '/hike.png'
var eat = '/eat.png'
var camp = '/camp.png'
var sightsee = '/sightsee.png'

var marker=new google.maps.Marker({
  position:place,
  icon:eat

    });
var marker1=new google.maps.Marker({
  position:place1,
  icon:hike

    });

var marker2=new google.maps.Marker({
   position:place2,
   icon:camp

    });
var marker3=new google.maps.Marker({
position:place3,
    icon: hike

    });
var marker4=new google.maps.Marker({
position:place4,
    icon:camp

    });
var marker5=new google.maps.Marker({
position:place5,
    icon:hike

    });
var marker6=new google.maps.Marker({
position:place6,
    icon:sightsee

    });






marker.setMap(map);
marker1.setMap(map);
marker2.setMap(map);
marker3.setMap(map);
marker4.setMap(map);
marker5.setMap(map);
marker6.setMap(map);


var infowindow = new google.maps.InfoWindow({
  content:"This is in Alaska. Where it's cold."
  });



google.maps.event.addListener(marker, 'click', function() {
  infowindow.open(map,marker);
  });




var myTrip = [place,place1,place2,place3,place4,place5,place6];
var flightPath = new google.maps.Polyline({
  path:myTrip,
  strokeColor:"#0000FF",
  strokeOpacity:0.8,
  strokeWeight:2
});
flightPath.setMap(map);
}

google.maps.event.addDomListener(window, 'load', initialize);
</script>

<div id="googleMap" style="width:500px;height:380px;"></div>
