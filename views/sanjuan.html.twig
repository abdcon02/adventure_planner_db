<!DOCTYPE html>
<html>
<head>
<script
src="http://maps.googleapis.com/maps/api/js">
</script>

<script>
var place1 =new google.maps.LatLng(48.506000, -122.677990);
var place2 =new google.maps.LatLng (48.595088, -122.940288);
var place3 =new google.maps.LatLng (48.703962, -122.915569);
var place4 =new google.maps.LatLng (48.622328, -122.835918);
var place5 =new google.maps.LatLng(48.544200, -123.015819);
var place6 =new google.maps.LatLng(48.596904, -123.100963);
var place7 =new google.maps.LatLng(48.465950, -123.047405);
var place8 =new google.maps.LatLng(48.554200, -122.893596);


function initialize()
{


var mapProp = {
  center:place2,
  zoom:10,
  mapTypeId:google.maps.MapTypeId.ROADMAP
  };

var map=new google.maps.Map(document.getElementById("googleMap"),mapProp);
var sail = 'sail.png'
var kayak = 'kayak.png'
var sightsee = 'sightsee.png'

var marker1=new google.maps.Marker({
  position:place1,
  icon: sail

});

var marker2=new google.maps.Marker({
  position:place2,
  icon: sail

  });

var marker3=new google.maps.Marker({
   position:place3,
   icon: sail

   });
var marker4=new google.maps.Marker({
    position:place4,
    icon:kayak

    });
var marker5=new google.maps.Marker({
    position:place5,
    icon:kayak

    });
var marker6=new google.maps.Marker({
    position:place6,
    icon:sightsee

    });
var marker7=new google.maps.Marker({
    position:place7,
    icon:sightsee

    });
var marker8=new google.maps.Marker({
    position:place8,
    icon:kayak

    });





marker1.setMap(map);
marker2.setMap(map);
marker3.setMap(map);
marker4.setMap(map);
marker5.setMap(map);
marker6.setMap(map);
marker7.setMap(map);
marker8.setMap(map);


var myTrip = [place1, place2, place3];
var flightPath = new google.maps.Polyline({
  path:myTrip,
  strokeColor:"#0000FF",
  strokeOpacity:0.8,
  strokeWeight:2
});



var myTrip=[place1, place2, place3, place4, place5, place6, place7, place8];
var flightPath=new google.maps.Polyline({
  paths:myTrip,
  strokeColor:"#DC143C",
  strokeOpacity:0.8,
  strokeWeight:2
  });

flightPath.setMap(map);
}

google.maps.event.addDomListener(window, 'load', initialize);
</script>
</head>

<body>
<div id="googleMap" style="width:500px;height:380px;"></div>
</body>
</html>
